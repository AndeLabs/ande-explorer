{
  "name": "ande-chain-explorer",
  "version": "1.0.0",
  "description": "Professional blockchain explorer for Ande Chain with Vercel deployment",
  "main": "public/index.html",
  "scripts": {
    "dev": "cd frontend && npm run dev",
    "build": "cd frontend && npm install && npm run build",
    "start": "cd frontend && npm start",
    "install": "cd frontend && npm install",
    "deploy": "vercel --prod",
    "clean": "rm -rf .vercel && vercel clean"
  },
  "keywords": [
    "blockchain",
    "explorer",
    "ande-chain",
    "ethereum",
    "vercel",
    "crypto",
    "defi",
    "web3"
  ],
  "author": {
    "name": "Ande Labs",
    "email": "contact@ande.chain",
    "url": "https://ande.chain"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AndeLabs/ande-explorer.git"
  },
  "homepage": "https://explorer.ande.chain",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "devDependencies": {
    "vercel": "^32.4.1"
  },
  "dependencies": {}
}