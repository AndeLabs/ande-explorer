{
  "framework": "nextjs",
  "env": {
    "NEXT_PUBLIC_API_HOST": "https://api.ande.network",
    "NEXT_PUBLIC_API_BASE_PATH": "/api/v2",
    "NEXT_PUBLIC_APP_HOST": "explorer.ande.network",
    "NEXT_PUBLIC_APP_PROTOCOL": "https",
    "NEXT_PUBLIC_NETWORK_NAME": "ANDE Network",
    "NEXT_PUBLIC_NETWORK_SHORT_NAME": "ANDE",
    "NEXT_PUBLIC_NETWORK_ID": "6174",
    "NEXT_PUBLIC_CHAIN_ID": "6174",
    "NEXT_PUBLIC_NETWORK_CURRENCY_NAME": "ANDE",
    "NEXT_PUBLIC_NETWORK_CURRENCY_SYMBOL": "ANDE",
    "NEXT_PUBLIC_NETWORK_CURRENCY_DECIMALS": "18",
    "NEXT_PUBLIC_IS_L2_NETWORK": "false",
    "NEXT_PUBLIC_IS_TESTNET": "false",
    "NEXT_PUBLIC_SHOW_GAS_TRACKER": "true",
    "NEXT_PUBLIC_HIDE_INDEXING_ALERT": "false",
    "NEXT_PUBLIC_DEFAULT_THEME": "dark",
    "NEXT_PUBLIC_RPC_URL": "https://rpc.ande.network",
    "NEXT_PUBLIC_WS_URL": "wss://api.ande.network",
    "NEXT_PUBLIC_API_URL": "https://api.ande.network/api",
    "NEXT_PUBLIC_ENABLE_WEBSOCKETS": "true",
    "NEXT_PUBLIC_FAUCET_URL": "https://faucet.ande.network",
    "NEXT_PUBLIC_WEBSITE_URL": "https://www.ande.network",
    "NEXT_PUBLIC_GITHUB_URL": "https://github.com/AndeLabs/ande-chain",
    "NEXT_PUBLIC_SHOW_COIN_PRICE": "false",
    "NEXT_PUBLIC_SHOW_MARKET_CAP": "false",
    "NEXT_PUBLIC_SHOW_NETWORK_ABOUT": "true",
    "NEXT_PUBLIC_SHOW_NETWORK_INFO": "true",
    "NEXT_PUBLIC_NETWORK_DESCRIPTION": "ANDE Chain is a sovereign EVM rollup built on Evolve with Celestia DA integration. Features include Token Duality Precompile, BFT Multi-Validator Consensus, and MEV Redistribution (80% stakers, 20% treasury).",
    "BACKEND_URL": "https://api.ande.network"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "geolocation=(), microphone=(), camera=()"
        }
      ]
    },
    {
      "source": "/_next/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/site.webmanifest",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/manifest+json"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    },
    {
      "source": "/(logo-.*\\.png|favicon\\.ico)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=604800, immutable"
        }
      ]
    },
    {
      "source": "/fonts/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "regions": ["gru1"],
  "rewrites": [
    {
      "source": "/api/rpc/:path*",
      "destination": "https://rpc.ande.network/:path*"
    }
  ]
}
