# ANDE Explorer - Production Environment
# Connected to live ANDE Chain at rpc.ande.network

# Chain Configuration
NEXT_PUBLIC_CHAIN_ID=6174
NEXT_PUBLIC_CHAIN_NAME=ANDE Network
NEXT_PUBLIC_NETWORK_CURRENCY=ANDE
NEXT_PUBLIC_NETWORK_CURRENCY_DECIMALS=18
NEXT_PUBLIC_IS_L2_NETWORK=true

# API Endpoints (Using live ANDE Chain)
# API base URL - no version prefix in configuration
NEXT_PUBLIC_API_HOST=https://api.ande.network/api
NEXT_PUBLIC_WS_URL=wss://ws.ande.network
NEXT_PUBLIC_RPC_URL=https://rpc.ande.network

# Explorer URLs
NEXT_PUBLIC_EXPLORER_URL=https://explorer.ande.network
NEXT_PUBLIC_ADVANCED_EXPLORER_URL=https://explorer.ande.network

# App Configuration
NEXT_PUBLIC_APP_NAME=ANDE Explorer
NEXT_PUBLIC_APP_VERSION=1.0.0
NEXT_PUBLIC_APP_DESCRIPTION=Official Block Explorer for ANDE Network - Sovereign Rollup with Evolve EVM

# Features
NEXT_PUBLIC_SHOW_GAS_TRACKER=true
NEXT_PUBLIC_ENABLE_WEBSOCKETS=true
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_DEFAULT_THEME=dark

# Social Links
NEXT_PUBLIC_TWITTER_URL=https://twitter.com/andechain
NEXT_PUBLIC_DISCORD_URL=https://discord.gg/andechain
NEXT_PUBLIC_GITHUB_URL=https://github.com/AndeLabs
NEXT_PUBLIC_DOCS_URL=https://docs.ande.network