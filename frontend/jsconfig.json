{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/lib/config": ["lib/config"],
      "@/lib/api": ["lib/api"],
      "@/lib/hooks": ["lib/hooks"],
      "@/lib/utils": ["lib/utils"],
      "@/lib/*": ["lib/*"],
      "@/components/*": ["components/*"],
      "@/app/*": ["app/*"],
      "@/styles/*": ["styles/*"],
      "@/*": ["./*"]
    }
  },
  "include": ["**/*"],
  "exclude": ["node_modules", ".next"]
}
